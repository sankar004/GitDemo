import java.util.ArrayList;

public class Test {
	
	public void streamFilter() {
		
		ArrayList<String> names =new ArrayList<String>();
		names.add("Muthu");
		names.add("Abijit");
		names.add("Sankar");
		names.add("Appadurai");
		names.add("Anu");

		System.out.println(names.stream().filter(s -> s.startsWith("A")).count());
		
		
	}

}
